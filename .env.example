# Supabase Configuration
# Example environment file with updated key formats

# Your Supabase project URL
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url_here

# Your Supabase publishable API key
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_publishable_key_here
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_KEY=your_supabase_publishable_key_here

# Optional secret key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_secret_key_here
SUPABASE_SECRET_KEY=your_supabase_secret_key_here

# Application Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Enable debug mode for development
NEXT_PUBLIC_DEBUG=false

